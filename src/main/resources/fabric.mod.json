{
	"schemaVersion": 1,
	"id": "kubos-tools",
	"version": "${version}",
	"name": "Kubo's Tools",
	"description": "A mod that adds things I made. I hope they're useful.",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/kubos-tools/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.kubo.KubosTools"
		],
		"client": [
			"net.kubo.KubosToolsClient"
		],
		"fabric-datagen": [
			"net.kubo.KubosToolsDataGenerator"
		]
	},
	"mixins": [
		"kubos-tools.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.5",
		"minecraft": "~1.19.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}